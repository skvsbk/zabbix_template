            <discovery_rules>
                <discovery_rule>
                    <name>Cartridges discovery</name>
                    <type>EXTERNAL</type>
                    <key>get_ink_level_epson.py[{HOST.CONN}, &quot;1&quot;]</key>
                    <delay>60m</delay>
                    <item_prototypes>
                        <item_prototype>
                            <name>Remaining in cartridge ({#COLOR} NO)</name>
                            <type>DEPENDENT</type>
                            <key>tonerpercentNO[{#COLOR}]</key>
                            <delay>0</delay>
                            <units>%</units>
                            <applications>
                                <application>
                                    <name>Counters (cartridges)</name>
                                </application>
                            </applications>
                            <preprocessing>
                                <step>
                                    <type>JSONPATH</type>
                                    <params>$.{#COLOR}</params>
                                </step>
                            </preprocessing>
                            <master_item>
                                <key>get_ink_level_epson.py[{HOST.CONN}, &quot;2&quot;]</key>
                            </master_item>
                            <trigger_prototypes>
                                <trigger_prototype>
                                    <expression>{last()}=2</expression>
                                    <recovery_mode>RECOVERY_EXPRESSION</recovery_mode>
                                    <recovery_expression>{last()}&gt;2</recovery_expression>
                                    <name>Less than 2% ink remaining ({#COLOR} NO) in {HOST.NAME}</name>
                                    <priority>HIGH</priority>
