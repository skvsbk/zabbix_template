            <items>
                <item>
                    <name>Software version</name>
                    <type>SNMP_AGENT</type>
                    <snmp_oid>.1.3.6.1.2.1.2.2.1.2.1</snmp_oid>
                    <key>firmware</key>
                    <delay>1d</delay>
                    <trends>0</trends>
